public class TypeCastingExample {
    public static void main(String[] args) {
        int num1 = 10;
        double num2 = num1; 
        System.out.println("Implicit casting: " + num2); 
        
        double num3 = 15.75;
        int num4 = (int) num3; 
        System.out.println("Explicit casting: " + num4); 
    }
}
